{
    "name": "deltoro/onto360",
    "description": "Web gestión de ontologías",
    "type": "project",
    "authors": [
        {
            "name": "ideltoro"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.47"
    },
    "repositories": {
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "scripts": {
        "server-frontend": "php -S localhost:8090 -t frontend",
        "server-backend": "php -S localhost:8081 -t backend"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "require-dev": {
        "yiisoft/yii2-debug": "^2.1",
        "yiisoft/yii2-gii": "^2.2"
    }
}